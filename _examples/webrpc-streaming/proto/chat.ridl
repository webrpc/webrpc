webrpc = v1

name = webrpc-sse-chat
version = v1.0.0

struct Message
  - id: uint64
  - username: string
  - text: string
  - createdAt: timestamp

service Chat
  - SendMessage(username: string, text: string)
  - SubscribeMessages(username: string) => stream (message: Message)

error 100 EmptyUsername "Username must be provided." HTTP 400
