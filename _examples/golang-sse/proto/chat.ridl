webrpc = v1

name = webrpc-sse-chat
version = v1.0.0

struct Message
  - id: uint64
  - username: string
  - text: string
  - createdAt: timestamp

service Chat
  - SendMessage(username: string, text: string)
  - SubscribeMessages(username: string) => stream (messages: Message)

error 100 ErrEmptyUsername "Username can't be empty." HTTP 400