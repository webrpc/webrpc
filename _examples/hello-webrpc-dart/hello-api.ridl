webrpc = v1

name = hello-webrpc
version = v1.0.0

enum Kind: uint32
  - USER = 1
  - ADMIN = 2

message User
  - ID: uint64
    + json = id
    + go.tag.db = id

  - username: string
    + json = USERNAME
    + go.tag.db = username

  - role: Kind

  - meta: map<string,any>

  - internalID: uint64
    + json = -

  - createdAt?: timestamp
    + json = created_at,omitempty
    + go.tag.db = created_at

message Page
  - num: uint32

service ExampleService
  - Ping() => (status: bool)
  - GetUser(userID: uint64) => (user: User)
  - FindUsers(q: string) => (page: Page, users: []User)
