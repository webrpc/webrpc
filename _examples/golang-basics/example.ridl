webrpc = v1             # version of webrpc schema format (ridl or json)

name = example          # name if your backend app
version = v0.0.1        # version of your app


enum Kind: uint32
  - USER
  - ADMIN


message Empty


message User
  - id: uint64
    + json = id
    + go.field.name = ID
    + go.tag.db = id

  - username: string
    + json = USERNAME
    + go.tag.db = username

  - role: string
    + go.tag.db = -

  - createdAt?: timestamp
    + json = created_at
    + go.tag.json = created_at,omitempty
    + go.tag.db = created_at


message ComplexType
  - meta: map<string,any>
  - metaNestedExample: map<string,map<string,uint32>>
  - namesList: []string
  - numsList: []int64
  - doubleArray: [][]string
  - listOfMaps: []map<string,uint32>
  - listOfUsers: []User
  - mapOfUsers: map<string,User>
  - user: User


service ExampleService
  - Ping()
  - Status() => (status: bool)
  - GetUser(header: map<string,string>, userID: uint64) => (code: uint32, user: User)
