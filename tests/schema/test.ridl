webrpc = v1             # version of webrpc schema format (ridl or json)

name = Test             # name of your backend app
version = v0.0.1        # version of your schema

struct Empty

struct User
  - id: uint64
    + json = id
    + go.field.name = ID
    + go.tag.db = id

  - username: string
    + json = USERNAME
    + go.tag.db = username

  - role: string
    + go.tag.db = -

enum Status: uint32
  - AVAILABLE
  - NOT_AVAILABLE

struct ComplexType
  - meta: map<string,any>
  - metaNestedExample: map<string,map<string,uint32>>
  - namesList: []string
  - numsList: []int64
  - doubleArray: [][]string
  - listOfMaps: []map<string,uint32>
  - listOfUsers: []User
  - mapOfUsers: map<string,User>
  - user: User
  - enum: Status

service ComplexApi
  - GetComplex() => (complex: ComplexType)
  - SendComplex(complex: ComplexType) => ()

service ErrorApi
  - GetError() => (empty: Empty)
